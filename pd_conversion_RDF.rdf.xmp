  <ns0:DiagnosticProcedure rdf:about="http://dayhoff.inf.um.es/graph/parkinson/resource/RT_QuIC">
    <rdfs:label xml:lang="es">RT-QuIC</rdfs:label>
    <ns0:description xml:lang="es">Amplificación de semillas de proteínas mal plegadas</ns0:description>
    <ns0:detects rdf:resource="http://dayhoff.inf.um.es/graph/parkinson/resource/AgregacionSinucleina"/>
    <ns0:has_input>
      <ns0:AnatomicalEntity rdf:about="http://dayhoff.inf.um.es/graph/parkinson/resource/LiquidoCefalorraquideo">
        <rdfs:label xml:lang="es">Líquido Cefalorraquídeo</rdfs:label>
        <owl:sameAs rdf:resource="http://purl.obolibrary.org/obo/UBERON_0001359"/>
      </ns0:AnatomicalEntity>
    </ns0:has_input>

  </ns0:DiagnosticProcedure>

  <ns0:Gene rdf:about="http://dayhoff.inf.um.es/graph/parkinson/resource/SNCA">
