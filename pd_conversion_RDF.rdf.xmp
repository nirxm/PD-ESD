  </ns0:PathologicalProcess>

  <ns0:PathologicalProcess rdf:about="http://dayhoff.inf.um.es/graph/parkinson/resource/DisfuncionMitocondrial">
    <rdfs:label xml:lang="es">Disfunción Mitocondrial</rdfs:label>
    <ns0:causes rdf:resource="http://dayhoff.inf.um.es/graph/parkinson/resource/PD"/>
    <ns0:correlated_with rdf:resource="http://dayhoff.inf.um.es/graph/parkinson/resource/EstresOxidativo"/>
  </ns0:PathologicalProcess>

  <ns0:PathologicalProcess rdf:about="http://dayhoff.inf.um.es/graph/parkinson/resource/FalloLisosomal">
    <rdfs:label xml:lang="es">Disfunción Lisosomal</rdfs:label>
