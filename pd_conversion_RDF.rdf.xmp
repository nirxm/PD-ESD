        <ns0:participates_in rdf:resource="http://dayhoff.inf.um.es/graph/parkinson/resource/Neuroinflamacion"/>
        <ns0:expressed_in rdf:resource="http://dayhoff.inf.um.es/graph/parkinson/resource/Microglia"/>
      </ns0:Protein>
    </ns0:has_gene_product>

