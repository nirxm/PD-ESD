  <rdf:Description rdf:about="http://dayhoff.inf.um.es/graph/parkinson/resource/PD">
    <rdf:type rdf:resource="https://w3id.org/biolink/vocab/Disease"/>
    <rdfs:label xml:lang="es">Enfermedad de Parkinson</rdfs:label>
    <ns0:description xml:lang="es">Trastorno neurodegenerativo caracterizado por la pérdida de neuronas dopaminérgicas y acumulación de cuerpos de Lewy.</ns0:description>
    <ns0:has_phenotype>
      <ns0:PhenotypicFeature rdf:about="http://dayhoff.inf.um.es/graph/parkinson/resource/Bradicinesia">
        <rdfs:label xml:lang="es">Bradicinesia</rdfs:label>
        <owl:sameAs rdf:resource="http://purl.obolibrary.org/obo/HP_0002067"/>
      </ns0:PhenotypicFeature>
    </ns0:has_phenotype>

    <ns0:has_phenotype rdf:resource="http://dayhoff.inf.um.es/graph/parkinson/resource/TemblorRepos"/>
    <ns0:has_phenotype>
      <ns0:PhenotypicFeature rdf:about="http://dayhoff.inf.um.es/graph/parkinson/resource/Rigidez">
        <rdfs:label xml:lang="es">Rigidez muscular</rdfs:label>
        <owl:sameAs rdf:resource="http://purl.obolibrary.org/obo/HP_0002063"/>
      </ns0:PhenotypicFeature>
    </ns0:has_phenotype>

    <ns0:has_phenotype>
      <ns0:PhenotypicFeature rdf:about="http://dayhoff.inf.um.es/graph/parkinson/resource/Depresion">
        <rdfs:label xml:lang="es">Depresión</rdfs:label>
        <ns0:category xml:lang="es">Sintoma no motor</ns0:category>
      </ns0:PhenotypicFeature>
    </ns0:has_phenotype>

    <ns0:has_phenotype>
      <ns0:PhenotypicFeature rdf:about="http://dayhoff.inf.um.es/graph/parkinson/resource/TrastornoSueno">
        <rdfs:label xml:lang="es">Trastornos del sueño</rdfs:label>
        <owl:sameAs rdf:resource="http://purl.obolibrary.org/obo/HP_0002360"/>
      </ns0:PhenotypicFeature>
    </ns0:has_phenotype>

    <ns0:has_phenotype>
      <ns0:PhenotypicFeature rdf:about="http://dayhoff.inf.um.es/graph/parkinson/resource/DeterioroCognitivo">
        <rdfs:label xml:lang="es">Deterioro cognitivo</rdfs:label>
        <owl:sameAs rdf:resource="http://purl.obolibrary.org/obo/HP_0100543"/>
      </ns0:PhenotypicFeature>
    </ns0:has_phenotype>

    <rdfs:seeAlso rdf:resource="http://purl.obolibrary.org/obo/MONDO_0005180"/>
  </rdf:Description>

  <ns0:PhenotypicFeature rdf:about="http://dayhoff.inf.um.es/graph/parkinson/resource/TemblorRepos">
    <rdfs:label xml:lang="es">Temblor en reposo</rdfs:label>
