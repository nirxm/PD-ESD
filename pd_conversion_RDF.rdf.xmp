            <owl:sameAs rdf:resource="http://purl.obolibrary.org/obo/GO_0045202"/>
          </ns0:CellularComponent>
        </ns0:expressed_in>

      </ns0:Protein>
    </ns0:has_gene_product>

  </ns0:Gene>

