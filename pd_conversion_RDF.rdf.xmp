    <owl:sameAs rdf:resource="http://purl.obolibrary.org/obo/HP_0002172"/>
  </ns0:PhenotypicFeature>

  <ns0:PathologicalProcess rdf:about="http://dayhoff.inf.um.es/graph/parkinson/resource/AgregacionSinucleina">
    <rdfs:label xml:lang="es">Agregación de Alpha-sinucleína (Cuerpos de Lewy)</rdfs:label>
    <ns0:causes rdf:resource="http://dayhoff.inf.um.es/graph/parkinson/resource/PD"/>
    <ns0:occurs_in rdf:resource="http://dayhoff.inf.um.es/graph/parkinson/resource/SustanciaNegra"/>
  </ns0:PathologicalProcess>

  <ns0:PathologicalProcess rdf:about="http://dayhoff.inf.um.es/graph/parkinson/resource/PerdidaNeuronal">
    <rdfs:label xml:lang="es">Pérdida de neuronas dopaminérgicas</rdfs:label>
